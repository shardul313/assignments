
using System;
class HelloWorld {
  static void Main() {
    Console.WriteLine("enter number ");
    int num = int.Parse(Console.ReadLine());
    int l=num;
    int rev=0,rem;
    while(l!=0){
        rem=l%10;
        rev=rev*10+rem;
        l=l/10;
        
    }
    Console.WriteLine($"reverse of {num} is {rev} ");
  }
}
