
using System;
class HelloWorld {
  static void Main() {
    Console.WriteLine("enter a number");
    int n= int.Parse(Console.ReadLine());
    
     for(int i=1;i<=n;i++){
         for(int j=1;j<=n;j++){
             if(j==i)
             {
             Console.Write("*");
            }
             else
             {
                 Console.Write(j);
                 
             }
         }
         Console.WriteLine();
     }
  }
}
    
