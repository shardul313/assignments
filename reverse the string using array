using System;
class HelloWorld {
  static void Main() {
    Console.WriteLine("enter a number");
    string n= Console.ReadLine();
    int t=n.Length-1;
    char[] arr = new char[n.Length];
    for(int i=0;i<arr.Length;i++){
        
        arr[i]=n[i];
    }
    for(int i=n.Length;i>0;i--){Console.Write(arr[i-1]);}
    
  }
}       
